from time import time

def fib(n):
    if n < 1:
        return 0
    if n == 1:
        return 1
    return fib(n - 1) + fib(n - 2)

def test_fib(n):
    t = time()
    f = fib(n)
    print ("Fib[" + str(n) + "] = ", f, "Timing :", str(time() - t) + "s\n")
    
def test_fibs(m, n):
    for k in rangeï¼ˆm, n):
        test_fib(k)
        
print(test_fibs(32,40))
